{
  "name": "studay-rust",
  "version": "1.0.0",
  "description": "学习rust",
  "author": "dengbo <1716775221@qq.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "doc": "vitepress dev docs --host",
    "docs:serve": "vitepress serve docs",
    "docs:build": "vitepress build docs",
    "cz": "npm run log && git add . && git cz",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.24.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "conventional-changelog-cli": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.3",
    "markdown-it": "^13.0.2",
    "markdown-it-mathjax3": "^4.3.2",
    "sass": "^1.77.8",
    "search-insights": "^2.15.0",
    "vitepress": "1.0.0-rc.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "engines": {
    "node": ">= 16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dengBox/studay-rust"
  },
  "bugs": {
    "url": "https://github.com/dengBox/studay-rust/issues"
  },
  "homepage": "https://dengbox.github.io/drop-manger",
  "dependencies": {
    "vite": "^4.5.3"
  }
}
